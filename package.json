{
  "name": "retta-and-adri",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:client": "cd ./client && npm i && npm run build && mkdir -p ../dist/client/build; cp -r ./build ../dist/client",
    "build:server": "tsc && mkdir -p ./dist/queries/sql && cp -r ./queries/sql/ ./dist/queries/sql/.",
    "build:server:heroku": "tsc && mkdir -p ./dist/queries/sql && cp -r ./queries/sql/ ./dist/queries/.",
    "build": "npm run build:server:heroku && npm run build:client",
    "preserver": "npm run build",
    "server": "node ./bin/www",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "connection-string": "^4.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pg-promise": "^11.13.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.15",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  }
}
